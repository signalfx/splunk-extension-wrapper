{
  "FunctionName": "${FUNCTION_NAME}",
  "Runtime": "nodejs12.x",
  "Handler": "index.handler",
  "Role": "arn:aws:iam::906383545488:role/signalfx-extension-wrapper-testing",
  "Code": {
    "ZipFile": "${FUNCTION_ZIP}"
  },
  "Environment": {
    "Variables": {
      "SPLUNK_REALM": "${SPLUNK_REALM}",
      "SPLUNK_ACCESS_TOKEN": "${SPLUNK_ACCESS_TOKEN}",
      "FAST_INGEST": "${FAST_INGEST}",
      "REPORTING_RATE": "1",
      "VERBOSE": "true"
    }
  },
  "Layers": [
    "${FUNCTION_LAYER}"
  ]
}
